<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="annual_review.annual_meeting.type.class">LinkR\Workflow\AnnualReviewBundle\Form\Type\AnnualMeetingNodeType</parameter>
        <parameter key="annual_review.annual_meeting.handler.class">LinkR\Workflow\AnnualReviewBundle\Form\Handler\AnnualMeetingNodeHandler</parameter>
    </parameters>

    <services>
        <!-- Form Type -->
        <service id="annual_review.annual_meeting.type" class="%annual_review.annual_meeting.type.class%" parent="LinkR.node.abstract_type">
            <tag name="form.type" alias="annual_review_annual_meeting_form" />
        </service>

        <!-- Form Handler -->
        <service id="annual_review.annual_meeting.handler" class="%annual_review.annual_meeting.handler.class%" parent="LinkR.node.abstract_handler">

        </service>

    </services>

</container>
