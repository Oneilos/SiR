<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="LinkR_task.domain.task.class">LinkR\Bundle\TaskBundle\Domain\TaskDomain</parameter>
    </parameters>

    <services>

        <!-- Task domain -->
        <service id="LinkR_task.domain.task" class="%LinkR_task.domain.task.class%">
            <argument type="service" id="LinkR_task.tools.temporal" />
        </service>

    </services>

</container>
